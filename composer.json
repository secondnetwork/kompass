{
    "name": "secondnetwork/kompass",
    "description": "Kompass A Laravel CMS",
    "keywords": [
        "laravel",
        "admin",
        "panel",
        "cms"
    ],
    "homepage": "https://github.com/secondnetwork/kompass",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Farah",
            "email": "github@secondnetwork.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.5.2",
        "illuminate/support": "^9.52.4|^10.21",
        "intervention/image": "^2.7.2",
        "laravel/fortify": "^1.17.5",
        "livewire/livewire": "^3.0.1",
        "secondnetwork/blade-tabler-icons": "^2.32",
        "symfony/http-client": "^6.3.2",
        "symfony/mailgun-mailer": "^6.3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23.0",
        "laravel/pint": "^0.2.4 || ^1.12.0",
        "mockery/mockery": "^1.6.6",
        "phpunit/phpunit": "^9.6.5 || ^10.3.2",
        "spatie/laravel-ignition": "^1.6.4 || ^2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Secondnetwork\\Kompass\\": "src",
            "Secondnetwork\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Secondnetwork\\Kompass\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "platform-check": false
    },
    "minimum-stability": "beta",
    "extra": {
        "laravel": {
            "providers": [
                "Secondnetwork\\Kompass\\KompassServiceProvider"
            ],
            "aliases": {
                "Kompass": "Secondnetwork\\Kompass\\KompassFacade"
            }
        }
    }
}
