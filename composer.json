{
    "name": "secondnetwork/kompass",
    "description": "Kompass A Laravel CMS",
    "keywords": [
        "laravel",
        "admin",
        "panel",
        "cms"
    ],
    "homepage": "https://github.com/secondnetwork/kompass",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Farah",
            "email": "github@secondnetwork.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.5.3",
        "symfony/http-kernel": "^6.2|^7.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0",
        "intervention/image": "^2.7.2 || ^3.3.0",
        "kolossal-io/laravel-multiplex": "^1.0.1",
        "laravel/folio": "^1.1.5",
        "laravel/fortify": "^1.20.0",
        "livewire/livewire": "^3.3.5",
        "livewire/volt": "^1.6.1",
        "secondnetwork/blade-tabler-icons": "^2.46",
        "symfony/mailgun-mailer": "^6.4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.9",
        "mockery/mockery": "^1.6.7",
        "phpunit/phpunit": "^10.5.8",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-ignition": "^2.4.1"
    },
    "autoload": {
        "psr-4": {
            "Secondnetwork\\Kompass\\": "src",
            "Secondnetwork\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Secondnetwork\\Kompass\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Secondnetwork\\Kompass\\KompassServiceProvider"
            ],
            "aliases": {
                "Kompass": "Secondnetwork\\Kompass\\KompassFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
